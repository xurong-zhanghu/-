#include <stdio.h>
int main()
{
	int x;
	//scanf("%d",&x);
	
	x=12345;
	do {
		int d = x%10;        // 对每一位取余 
		printf("%d",d);      //不输出\n是为了将取余之数排在一行 
		
		if (x>9) {
			printf(" ")
		}                    //保证输出数字空格隔开，X>9是为了最后一个数字没空格 
		
		x /= 10;             //保证了每一数都取到 
	} while (x>0);
	printf("\n");
	return 0;
 } 
